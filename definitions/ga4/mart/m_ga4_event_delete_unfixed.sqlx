-- Config blocks
config {
    //typeは初期構築時はtable、増分更新時は"incremental"とする。増分更新時は最終行のコメントアウトも外すこと。
    type: "operations",
    schema: "df_molts_ga4_mart",
    description: "GA4マートデータ　イベント用",
    tags: ["daily", "mart", "deletion_query"],
}

-- Query blocks

DECLARE table_exists BOOL;

SET table_exists = (
  SELECT 
    COUNT(*) > 0 
  FROM 
    `${constants.GA4_DATABASE}.${constants.MART}.INFORMATION_SCHEMA.TABLES`
  WHERE 
    table_name = 'm_ga4_event'
);

IF table_exists THEN
  DELETE FROM `molts-data-project.df_molts_ga4_mart.m_ga4_event` WHERE is_fixed_data = false;
END IF;
